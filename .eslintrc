{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true,
    "jest": true
  },
  "rules": {
    "indent": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "operator-linebreak": "off",
    "linebreak-style": ["error", "windows"],
    "max-len": "off"

  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}